{
  "name": "contentful-table",
  "description": "A simple table Contentful UI extension",
  "version": "1.0.0",
  "scripts": {
    "build": "parcel build src/index.html",
    "prepublish": "npm run build",
    "publish:create": "npm run source-vars && contentful extension create --space-id ${SPACE_ID} --name ${EXTENSION_NAME}",
    "publish:update": "npm run source-vars && contentful extension update --space-id ${SPACE_ID} --name ${EXTENSION_NAME} --force",
    "source-vars": "./node_modules/.bin/env-cmd -f ./env",
    "start": "parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "https://github.com/pdq/contentful-table#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdq/contentful-table.git"
  },
  "bugs": {
    "url": "https://github.com/pdq/contentful-table/issues"
  },
  "dependencies": {
    "contentful-ui-extensions-sdk": "^3.10.7"
  },
  "devDependencies": {
    "@pdq/eslint-plugin-pdq": "^1.0.1",
    "@types/node": "^13.5.1",
    "contentful-cli": "^1.2.11",
    "env-cmd": "^10.0.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.25.1-test.1",
    "sortpack": "^2.1.1",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "package.json": [
      "sortpack"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
